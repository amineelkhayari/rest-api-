{
  "name": "yourname/php-api-starter",
  "description": "No-framework PHP API with clean architecture, router, middleware, Swagger UI",
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": ">=8.0",
    "doctrine/orm": "^3.5",
    "doctrine/dbal": "^4.3",
    "doctrine/migrations": "^3.9",
    "symfony/cache": "^7.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/App/",
      "Core\\": "src/Core/",
      "Routes\\": "src/Routes/",
       "Exceptions\\": "src/Exceptions/"
    }
  }
  ,
  "scripts": {
    "generate-openapi": "php scripts/generate_openapi.php"
  }
    ,
    "authors": [
      {
        "name": "amine",
        "email": "amine@example.com"
      }
    ],
    "minimum-stability": "stable"
}
